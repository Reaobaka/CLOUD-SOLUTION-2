@{
    ViewData["Title"] = "Home Page";

}
<style>

    .container {
        background-color: white;
        max-width: flex;
        height: 45px;
    }

    .menu {
        display: flex;
        align-items: center; /* Vertically center the items */
        width: 100%;
    }

    .logo {
        margin-right: 800px; /* Space between "ABC RETAIL" and the menu items */
        white-space: nowrap; /* Prevent "ABC RETAIL" from wrapping */
        font-weight: bold;
    }

    .menu-items {
        display: flex;
    }

    .menu-item {
        margin-right: 2px; /* Minimal space between menu items */
    }

        .menu-item a {
            color: black; /* Inherit text color */
            padding: 5px; /* Add some padding to the link for better click area */
            display: inline-block;
            font-weight: bold;
        }

        /* Remove margin from the last menu item */
        .menu-item:last-child {
            margin-right: 0;
        }

    .main-text {
        position: relative;
        display: flex;
        flex-direction: column; /* Aligns text blocks vertically */
        align-items: flex-start; /* Aligns the text to the left */
        justify-content: center; /* Centers the text vertically */
        height: 400px; /* Sets the height to 100% of the viewport */
        text-align: left; /* Aligns text to the left */
        color: black; /* Ensures the text is readable against the background */
        padding: 0 20px; /* Adds padding to the sides for better readability */
        margin: 0;
        background-image: url('https://www.shutterstock.com/image-illustration/clothes-on-grunge-background-shelf-600nw-1891025299.jpg'); /* Background image URL */
        background-size: cover;
        background-position: center;
        background-repeat: no-repeat;
        z-index: 1;
    }

        .main-text::before {
            content: '';
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            background-color: rgba(0, 0, 0, 0.5); /* Semi-transparent overlay */
            z-index: -1;
        }

        .main-text p {
            margin: 0; /* Removes default margin */
            padding: 0; /* Removes default padding */
            font-size: 1.5rem;
        }

        .main-text b {
            font-size: small; /* Sets the font size for bold text */
        }

        .main-text p:nth-of-type(2) {
            font-family: 'Times New Roman'; /* Sets the font family for the second paragraph */
            font-size: small; /* Sets the font size for the second paragraph */
        }

    .larger-text {
        font-size: 2em;
        line-height: 1.2;
    }

    .gold-text {
        font-weight: bold;
        color: antiquewhite;
    }

    .slogan {
        text-align: center;
        font-size: 1rem;
        background-color: white;
        max-width: flex;
        height: 45px;
        box-shadow: 2px 2px 5px rgba(0, 0, 0.3);
    }

    .container {
        display: flex; /* Use flexbox to align items in a row */
        flex-wrap: wrap; /* Allow items to wrap to the next line if necessary */
        gap: 50px; /* Space between items */
        padding: 10px; /* Padding around the container */
        justify-content: center; /* Center items horizontally */
    }

    /* Image item styling */
    .image-item {
        display: flex;
        flex-direction: column; /* Stack label and image vertically */
        align-items: center; /* Center items horizontally */
        text-align: center; /* Center text */
        background-color: white; /* Light background for the container */

        padding: 5px; /* Padding inside the container */
        /* Shadow for better visibility */
        max-width: 200px; /* Limit maximum width of each image item */


        transition: transform 0.3s ease, box-shadow 0.3s ease;
    }

        .image-item img {
            width: 200px; /* Fixed width for images */
            height: 150px; /* Fixed height for images */
            object-fit: cover; /* Ensures the image covers the container */
            border-radius: 5px; /* Rounded corners for images */
            transition: transform 0.3s ease;
        }

        .image-item:hover {
            transform: scale(1.1); /* Scales up the entire item */
            box-shadow: 0 4px 10px rgba(0, 0, 0, 0.2); /* Adds a more prominent shadow */
        }

        .image-item img:hover {
            transform: scale(1.05); /* Scales up the image slightly */
        }

        .image-item p {
            margin: 5px 0 0; /* Margin above the label */
            font-size: 0.9rem; /* Slightly smaller font size */
            color: #333; /* Darker color for the text */
            font-weight: bold;
        }
</style>
